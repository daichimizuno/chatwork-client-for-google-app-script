{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./dist/.diff",
    "target": "es2015",
    "module": "es2020",
    "rootDir": "./src",
    "baseUrl": "./",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist/js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "pretty": true,
  }
}
